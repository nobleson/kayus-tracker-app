<!doctype html>
<html lang="en">
  <head>
    <title>Radio</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <link href="theme-a/css/bootstrap.css" rel="stylesheet">
    <link href="theme-a/css/font-awesome.css" rel="stylesheet">
    <link href="theme-a/css/superslides.css" rel="stylesheet">
    <link href="theme-a/css/mediaelementplayer.css" rel="stylesheet">
    <link href="theme-a/Core/css/shiPlayer.css" rel="stylesheet">
    <link href="theme-a/css/style.css" rel="stylesheet">
    <link href="https://npmcdn.com/flickity@2/dist/flickity.css" rel="stylesheet">
    <style>
    body{
      overflow-x: hidden;
    }
    </style>
  </head>
  <body class="front">
    <div id="main">
      <div class="header-v1">
        <div class="container">
          <div class="header-inner clearfix">
            <header>
              <div class="logo_wrapper">
                <a href="index-2.html" class="logo">
                  <img src="theme-a/images/logo.png" alt="" class="img-fluid">
                </a>
              </div>
            </header>
            <ul class="social-top clearfix">
              <li><a href="#"><i class="fa fa-twitter"></i></a></li>
              <li><a href="#"><i class="fa fa-facebook"></i></a></li>
            </ul>
            <nav class="navbar_ navbar navbar-expand-md clearfix">
              <% include theme-a-navbar.ejs%>
            </nav>
          </div>
        </div>
      </div>
      <div id="home" class="">
        <div id="slides_wrapper" class="">
          <div id="slides">
            <ul class="slides-container">
              <li>
                <img src="theme-a/images/slide01.jpg" alt="" class="img">
                <div class="caption">
                  <div class="container">
                    <div class="img1"><img src="theme-a/images/slide01-logo.png" alt="" class="img-fluid preserve"></div>
                    <div class="txt1"><a href="/contact#content" class="btn1">Contact Us</a></div>
                  </div>
                </div>
              </li>
              <li>
                <img src="theme-a/images/slide02.jpg" alt="" class="img">
                <div class="caption">
                  <div class="container">
                    <div class="img1"><img src="theme-a/images/slide02-logo.png" alt="" class="img-fluid preserve"></div>
                    <div class="txt1"><a href="#" class="btn1">Try Listen</a></div>
                  </div>
                </div>
              </li>
              <li>
                <img src="theme-a/images/slide03.jpg" alt="" class="img">
                <div class="caption">
                  <div class="container">
                    <div class="img1"><img src="theme-a/images/slide03-logo.png" alt="" class="img-fluid preserve"></div>
                    <div class="txt1"><a href="#" class="btn2">Try Listen</a></div>
                  </div>
                </div>
              </li>
            </ul>
          </div>
        </div>
        <div class="mouse"><a href="#welcome" class="scroll-to"><img src="theme-a/images/mouse.png" alt="" class="img-responsive"></a></div>
        <div class="audio1-wrapper">
          <div class="container">
            <div class="audio1-inner">
              <div class="audio1-txt">
                <div class="txt1">Lalo (Jesica</div>
                <div class="txt2">Bernshtein) & Binno</div>
              </div>
              <div class="audio1">
                <audio controls="controls" preload="none" class="audio" style="width: 100%; visibility: hidden;">
                  <source type="audio/mpeg" src="theme-a/audio/song1.mp3"/>
                  <source type="audio/mpeg" src="theme-a/audio/song2.mp3"/>
                  <source type="audio/mpeg" src="theme-a/audio/song3.mp3"/>
                  <source type="audio/mpeg" src="theme-a/audio/song4.mp3"/>
                </audio>
              </div>
              <div class="audio1-cust-icon">
                <i class="fa fa-music" aria-hidden="true"></i>
              </div>
            </div>
          </div>
        </div>
      </div>
      <% include featured-channels.ejs%>
      <% include top-five-musics.ejs%>
      <% include featured-live-events.ejs%>
      <% include theme-a-footer.ejs%>
    </div>
    <script src="theme-a/js/jquery.min.js"></script>
    <script src="theme-a/js/jquery.easing.1.3.js"></script>
    <script src="theme-a/js/superfish.js"></script>
    <script src="theme-a/js/jquery.superslides.js"></script>
    <script src="https://npmcdn.com/flickity@2/dist/flickity.pkgd.js"></script>
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyA85u2f3GPNnJhIJeggViAfB8zMkUbCHNE"></script>
    <script src="theme-a/js/googlemap.js"></script>

    <script src="theme-a/js/mediaelement-and-player.js"></script>
    <script src="theme-a/js/mep-feature-playlist.js"></script>

    <script src="theme-a/js/Core/js/shiPlayer.js"></script>

    <script>
        $("#radio1").shiPlayer({
            type: "SHOUTCAST",
            URL: "https://streamingv2.shoutcast.com/JazzRadioBcn",
            radio_logo: "Core/img/radioLogo.jpg",
        });
        $("#radio2").shiPlayer({
            type: "icecast",
            URL: "https://streamingv2.shoutcast.com/JazzRadioBcn",
            mount_point: "chiptune.mp3",
            cors_proxy: "https://cors-anywhere.herokuapp.com/"
        });

    </script>

    <script src="theme-a/js/popper.min.js"></script>
    <script src="theme-a/js/bootstrap.min.js"></script>
    <script src="theme-a/js/scripts.js"></script>

  </body>
</html>
